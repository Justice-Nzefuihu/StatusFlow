{
  "data_api_version": "3.0",
  "routing_model": {
    "INDEX": [
      "ADD_STATUS",
      "VIEW_STATUS",
      "DELETE_STATUS",
      "NO_DATA"
    ],
    "ADD_STATUS": [
      "COMPLETE",
      "ERROR"
    ],
    "VIEW_STATUS": [
      "STATUS_DETAILS",
      "ERROR"
    ],
    "DELETE_STATUS": [
      "COMPLETE",
      "ERROR"
    ],
    "STATUS_DETAILS": [
      "UPDATE"
    ],
    "UPDATE": [
      "COMPLETE",
      "ERROR"
    ],
    "ERROR": [],
    "COMPLETE": [],
    "NO_DATA": []
  },
  "screens": [
    {
      "id": "INDEX",
      "title": "StatusFlow",
      "data": {
        "Choose_an_action": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "main-content": {
                "type": "object",
                "properties": {
                  "title": {
                    "type": "string"
                  }
                }
              }
            }
          },
          "__example__": [
            {
              "id": "ADD_STATUS",
              "main-content": {
                "title": "Add Status"
              },
              "on-click-action": {
                "name": "navigate",
                "next": {
                  "name": "ADD_STATUS",
                  "type": "screen"
                },
                "payload": {}
              }
            },
            {
              "id": "VIEW_STATUS",
              "main-content": {
                "title": "Get Status"
              },
              "on-click-action": {
                "name": "data_exchange",
                "payload": {
                  "action": "Get Status",
                  "selected": "View"
                }
              }
            },
            {
              "id": "DELETE_STATUS",
              "main-content": {
                "title": "Delete Status"
              },
              "on-click-action": {
                "name": "data_exchange",
                "payload": {
                  "action": "Get Status",
                  "selected": "Delete"
                }
              }
            }
          ]
        }
      },
      "layout": {
        "type": "SingleColumnLayout",
        "children": [
          {
            "type": "NavigationList",
            "name": "Choose_an_action",
            "list-items": "${data.Choose_an_action}"
          }
        ]
      }
    },
    {
      "id": "ADD_STATUS",
      "layout": {
        "children": [
          {
            "children": [
              {
                "type": "PhotoPicker",
                "name": "photo_picker",
                "label": "Upload photos",
                "description": "Please attach images if not only text status",
                "photo-source": "camera_gallery",
                "min-uploaded-photos": 1,
                "max-uploaded-photos": 1,
                "max-file-size-kb": 10240,
                "enabled": "`(${form.Only_Text} != true)`"
              },
              {
                "label": "Write up",
                "name": "Write_up",
                "required": "${form.Only_Text}",
                "type": "TextArea"
              },
              {
                "label": "Only Text",
                "name": "Only_Text",
                "required": false,
                "type": "OptIn"
              },
              {
                "type": "Dropdown",
                "label": "Schedule",
                "required": true,
                "name": "Schedule",
                "data-source": [
                  {
                    "id": "Every Day",
                    "title": "Every Day"
                  },
                  {
                    "id": "Every 2 Days",
                    "title": "Every 2 Days"
                  },
                  {
                    "id": "Every 3 Days",
                    "title": "Every 3 Days"
                  },
                  {
                    "id": "Every 4 Days",
                    "title": "Every 4 Days"
                  },
                  {
                    "id": "Every 5 Days",
                    "title": "Every 5 Days"
                  },
                  {
                    "id": "Every 6 Days",
                    "title": "Every 6 Days"
                  },
                  {
                      "id": "Every Week",
                      "title": "Every Week"
                    },
                    {
                      "id": "Every 10 Days",
                      "title": "Every 10 Days"
                    },
                    {
                      "id": "Every 2 Week",
                      "title": "Every 2 Week"
                    }
                ]
              },
              {
                "type": "Dropdown",
                "label": "Schedule time",
                "required": true,
                "name": "Schedule_time",
                "data-source": [
                  {
                    "id": "7:00:00",
                    "title": "7:00AM"
                  },
                  {
                    "id": "8:00:00",
                    "title": "8:00AM"
                  },
                  {
                    "id": "9:00:00",
                    "title": "9:00AM"
                  },
                  {
                    "id": "10:00:00",
                    "title": "10:00AM"
                  },
                  {
                    "id": "11:00:00",
                    "title": "11:00AM"
                  },
                  {
                    "id": "12:00:00",
                    "title": "12:00PM"
                  },
                  {
                    "id": "13:00:00",
                    "title": "1:00PM"
                  }
                ]
              },
              {
                "label": "Add",
                "on-click-action": {
                  "name": "data_exchange",
                  "payload": {
                    "image": "${form.photo_picker}",
                    "write_up": "${form.Write_up}",
                    "is_text": "${form.Only_Text}",
                    "schedule": "${form.Schedule}",
                    "schedule_time": "${form.Schedule_time}"
                  }
                },
                "type": "Footer"
              }
            ],
            "name": "add_status_form",
            "type": "Form"
          }
        ],
        "type": "SingleColumnLayout"
      },
      "title": "Add Status"
    },
    {
      "id": "VIEW_STATUS",
      "title": "View Status",
      "data": {
        "statuses": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "main-content": {
                "type": "object",
                "properties": {
                  "title": {
                    "type": "string"
                  },
                  "discription": {
                    "type": "string"
                  },
                  "metadata": {
                    "type": "string"
                  }
                }
              },
              "start": {
                "type": "object",
                "properties": {
                  "image": {
                    "type": "string"
                  }
                }
              }
            }
          },
          "__example__": [
            {
              "id": "b9a7b7f8-1234-4c6e-b001-abcde12345f6",
              "main-content": {
                "title": "Morning update",
                "description": "Schedule: 07:00 AM (Everyday)",
                "metadata": "created_at: 2025-10-28T13:10:00"
              },
              "start": {
                "image": ""
              },
              "on-click-action": {
                "name": "navigate",
                "next":{
                    "name":"STATUS_DETAILS",
                    "type":"screen"
                },
                "payload": {
                  "id": "b9a7b7f8-1234-4c6e-b001-abcde12345f6",
                  "Write_up": "Morning vibes ðŸŒ…",
                  "type": "Type: Text Status",
                  "image": "/9j/4AAQSkZJRgABAQAAAQABAADâ€¦",
                  "schedule":  "Schedule: EVERYDAY at 07:00 AM",
                  "is_upload":  "Uploaded: Yes",
                  "created_at": "Created At: 2025-10-28 at 11:43:10",
                  "upload_window_active": true
                }
              }
            }
          ]
        }
      },
      "layout": {
        "type": "SingleColumnLayout",
        "children": [
          {
            "type": "Form",
            "name": "view_status_form",
            "children": [
              {
                "type": "NavigationList",
                "name": "status",
                "label": "Status List",
                "description": "Choose your preferred Status to view",
                "list-items": "${data.statuses}"
              }
            ]
          }
        ]
      }
    },
    {
      "id": "DELETE_STATUS",
      "title": "Delete Status",
      "data": {
        "statuses": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "title": {
                "type": "string"
              },
              "discription": {
                "type": "string"
              },
              "metadata": {
                "type": "string"
              },
              "image": {
                "type": "string"
              }
            }
          },
          "__example__": [
            {
              "id": "b9a7b7f8-1234-4c6e-b001-abcde12345f6",
              "title": "Morning update",
              "description": "Schedule: 07:00 AM (Everyday)",
              "metadata": "created_at: 2025-10-28T13:10:00",
              "enabled": false,
              "image": "/9j/4AAQSkZJRgABAQAAAQABAADâ€¦"
            }
          ]
        }
      },
      "layout": {
        "type": "SingleColumnLayout",
        "children": [
          {
            "type": "Form",
            "name": "delete_status_form",
            "children": [
              {
                "type": "CheckboxGroup",
                "name": "delete_status",
                "label": "Delete Status",
                "description": "Choose your preferred Status to Delete (Status within its upload time can't be deleted)",
                "required": true,
                "data-source": "${data.statuses}"
              },
              {
                "type": "Footer",
                "label": "Delete",
                "on-click-action": {
                  "name": "data_exchange",
                  "payload": {
                    "selected_status": "${form.delete_status}"
                  }
                }
              }
            ]
          }
        ]
      }
    },
    {
      "data": {
        "id": {
          "type": "string",
          "__example__": "0e0e4e2b-8d52-4b4e-9f83-31adfb90dc33"
        },
        "write_up": {
          "type": "string",
          "__example__": "Morning vibes ðŸŒ…"
        },
        "type": {
          "type": "string",
          "__example__": "Type: Text Status"
        },
        "is_text": {
          "type": "boolean",
          "__example__": true
        },
        "image": {
          "type": "string",
          "__example__": "https://storage.googleapis.com/statusflow/status1.jpg"
        },
        "schedule": {
          "type": "string",
          "__example__": "Schedule: EVERYDAY at 07:00 AM"
        },
        "is_upload": {
          "type": "string",
          "__example__": "Uploaded: Yes"
        },
        "created_at": {
          "type": "string",
          "__example__": "Created At: 2025-10-28 at 11:43:10"
        },
        "upload_window_active": {
          "type": "boolean",
          "__example__": false
        }
      },
      "id": "STATUS_DETAILS",
      "layout": {
        "type": "SingleColumnLayout",
        "children": [
          {
            "type": "TextHeading",
            "text": "Your Status Details"
          },
          {
            "type": "Image",
            "visible": "`${data.is_text == false}`",
            "src": "${data.image}",
            "alt-text": "Status image"
          },
          {
            "type": "TextSubheading",
            "text": "Write Up"
          },
          {
            "type": "TextBody",
            "text": "${data.write_up}"
          },
          {
            "type": "TextBody",
            "text": "${data.type}"
          },
          {
            "type": "TextBody",
            "text": "${data.created_at}"
          },
          {
            "type": "TextBody",
            "text": "${data.schedule}"
          },
          {
            "type": "TextBody",
            "text": "${data.is_upload}"
          },
          {
            "type": "Form",
            "name": "Choose_an_action_for_detail_form",
            "children": [
                {
            "type": "If",
            "condition": "(!${data.upload_window_active})",
            "then": [
              {
                "data-source": [
                  {
                    "id": "Delete",
                    "title": "Delete"
                  },
                  {
                    "id": "Update",
                    "title": "Update"
                  }
                ],
                "label": "Choose an action",
                "name": "Choose_an_action_for_detail",
                "description": "Can't update/delete when it is within the upload time",
                "required": true,
                "type": "RadioButtonsGroup"
              },
              {
                "type": "Footer",
                "label": "confirm",
                "on-click-action": {
                  "name": "data_exchange",
                  "payload": {
                    "Choose_an_action_for_detail": "${form.Choose_an_action_for_detail}",
                    "status_id": "${data.id}",
                    "id": "${data.id}",
                    "Write_up": "${data.write_up}",
                            "type": "${data.type}",
                            "image": "${data.image}",
                            "schedule": "${data.schedule}",
                            "is_upload": "${data.is_upload}",
                            "created_at": "${data.created_at}",
                            "upload_window_active": "${data.upload_window_active}"
                  }
                }
              }
            ]
          }
            ]
          }
        ]
      },
      "title": "Status Details"
    },
    {
      "data": {
        "id": {
          "type": "string",
          "__example__": "0e0e4e2b-8d52-4b4e-9f83-31adfb90dc33"
        },
        "write_up": {
          "type": "string",
          "__example__": "Morning vibes ðŸŒ…"
        },
        "type": {
          "type": "string",
          "__example__": "Type: Text Status"
        },
        "is_text": {
          "type": "boolean",
          "__example__": true
        },
        "images_path": {
          "type": "string",
          "__example__": "https://storage.googleapis.com/statusflow/status1.jpg"
        },
        "schedule": {
          "type": "string",
          "__example__": "Everyday"
        },
        "schedule_time": {
          "type": "string",
          "__example__": "7:00AM"
        },
        "is_upload": {
          "type": "string",
          "__example__": "Uploaded: Yes"
        },
        "created_at": {
          "type": "string",
          "__example__": "Created At: 2025-10-28 at 11:43:10"
        },
        "upload_window_active": {
          "type": "boolean",
          "__example__": false
        }
      },
      "id": "UPDATE",
      "layout": {
        "children": [
          {
            "init-values": {
              "Write_up": "${data.write_up}",
              "Schedule": "${data.schedule}",
              "Schedule_time": "${data.schedule_time}"
            },
            "children": [
              {
                "type": "Image",
                "visible": "`${data.is_text == false}`",
                "src": "${data.images_path}",
                "alt-text": "Status image"
              },
              {
                "label": "Write up",
                "name": "Write_up",
                "required": "${data.is_text}",
                "type": "TextArea"
              },
              {
                "type": "Dropdown",
                "label": "Schedule",
                "required": true,
                "name": "Schedule",
                "data-source": [
                  {
                    "id": "Everyday",
                    "title": "Everyday"
                  },
                  {
                    "id": "Every_2_days",
                    "title": "Every 2 days"
                  },
                  {
                    "id": "Every_week",
                    "title": "Every week"
                  }
                ]
              },
              {
                "type": "Dropdown",
                "label": "Schedule time",
                "required": true,
                "name": "Schedule_time",
                "data-source": [
                  {
                    "id": "7:00AM",
                    "title": "7:00AM"
                  },
                  {
                    "id": "8:00AM",
                    "title": "8:00AM"
                  },
                  {
                    "id": "9:00AM",
                    "title": "9:00AM"
                  }
                ]
              },
              {
                "type": "TextBody",
                "text": "${data.type}"
              },
              {
                "type": "TextBody",
                "text": "${data.created_at}"
              },
              {
                "label": "Update",
                "on-click-action": {
                  "name": "data_exchange",
                  "payload": {
                    "status_id": "${data.id}",
                    "Write_up": "${form.Write_up}",
                    "schedule": "${form.Schedule}",
                    "schedule_time": "${form.Schedule_time}"
                  }
                },
                "type": "Footer"
              }
            ],
            "name": "add_status_form",
            "type": "Form"
          }
        ],
        "type": "SingleColumnLayout"
      },
      "title": "Update"
    },
    {
      "id": "ERROR",
      "title": "Error",
      "data": {
        "error_mssg": {
          "type": "string",
          "__example__": "Phone number already exists."
        }
      },
      "layout": {
        "type": "SingleColumnLayout",
        "children": [
          {
            "type": "RichText",
            "text": "${data.error_mssg}"
          }
        ]
      }
    },
    {
      "id": "NO_DATA",
      "title": "No Data",
      "layout": {
        "type": "SingleColumnLayout",
        "children": [
          {
            "type": "RichText",
            "text": "No Status available"
          },
          {
            "type": "Footer",
            "label": "Add Status",
            "on-click-action": {
              "name": "navigate",
              "next": {
                "name": "ADD_STATUS",
                "type": "screen"
              },
              "payload": {}
            }
          }
        ]
      }
    },
    {
      "id": "COMPLETE",
      "title": "Success!",
      "data": {
        "mssg": {
          "type": "string",
          "__example__": "Successfully added a status"
        }
      },
      "terminal": true,
      "layout": {
        "type": "SingleColumnLayout",
        "children": [
          {
            "type": "Form",
            "name": "flow_path",
            "children": [
              {
                "type": "Image",
                "src": "",
                "height": 84,
                "scale-type": "contain"
              },
              {
                "type": "TextSubheading",
                "text": "${data.mssg}"
              }
            ]
          }
        ]
      }
    }
  ],
  "version": "7.2"
}